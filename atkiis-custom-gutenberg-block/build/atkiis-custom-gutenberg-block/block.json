{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/atkiis-custom-gutenberg-block",
  "version": "0.1.0",
  "title": "Atkiis Custom Gutenberg Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "content": {
      "type": "string",
      "default": "Your content here...",
      "source": "html",
      "selector": "p"
    }
  },
  "supports": {
    "html": false,
    "color": {
      "text": true,
      "background": true
    }
  },
  "textdomain": "atkiis-custom-gutenberg-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}